<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F8F5FF"
    tools:context=".Home">

    <TextView
        android:id="@+id/AddPro"
        android:layout_width="281dp"
        android:layout_height="46dp"
        android:layout_marginTop="76dp"
        android:text="@string/companyName"
        android:textAlignment="center"
        android:textColor="#0B0000"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="326dp"
        android:layout_height="75dp"
        android:layout_marginTop="80dp"
        android:background="#FFFFFF"
        android:clickable="true"
        android:contextClickable="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/AddPro"
        app:srcCompat="@drawable/add_prescription"
        tools:srcCompat="@drawable/add_prescription" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="326dp"
        android:layout_height="75dp"
        android:layout_marginTop="24dp"
        android:clickable="true"
        android:contextClickable="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:srcCompat="@drawable/lab_reports"
        tools:srcCompat="@drawable/lab_reports" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="326dp"
        android:layout_height="75dp"
        android:layout_marginTop="24dp"
        android:clickable="true"
        android:contextClickable="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        app:srcCompat="@drawable/home_med"
        tools:srcCompat="@drawable/home_med" />


</androidx.constraintlayout.widget.ConstraintLayout>